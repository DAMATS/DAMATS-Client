<div class="panel-heading">
    <button type="button" class="close"><i class="fa fa-times-circle"></i></button>
    <h3 class="panel-title"><i class="fa fa-fw fa-gears"></i>&nbsp;Processes</h3>
</div>
<div class="panel-body">
<h5>Available Processes:</h5>
{{#if is_fetching }}
  <p class="text-danger" id="alert">
    <i class="fa fa-cog fa-spin"></i> Loading data ...
  </p>
{{else}}{{#if fetch_failed }}
  <p class="text-danger" id="alert">
    <i class="fa fa-warning"></i> Failed to load the data from the server!
  </p>
{{else}}{{#if is_empty }}
  <p class="text-danger" id="alert">
    No process avaialable.
  </p>
{{else}}
  <ul class="list-group process-list" id="process-list"></ul>
{{/if}}{{/if}}{{/if}}
</div>
